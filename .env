# Etsy app credentials
CLIENT_ID=your_etsy_client_id
CLIENT_SECRET=your_etsy_client_secret

# The URL Etsy will redirect back to (must match app settings)
REDIRECT_URI=https://yourdomain.com/auth/callback

# Your server base URL (for building redirect URL if needed)
BASE_URL=http://localhost:3000

# Shop id where you want to create listings (numeric)
SHOP_ID=12345678

# Demo token storage file (can be changed)
TOKEN_STORE=./tokens.json

PORT=3000
